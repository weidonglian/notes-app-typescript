{
  "name": "notesapp",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dev-start": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "dev-typeorm": "tsc && node ./node_modules/typeorm/cli.js --config src/config/ormconfig.dev",
    "prod-start": "tsc && APP_MODE=prod node ./dist/server.js",
    "prod-pm2": "tsc && APP_MODE=prod npx pm2 start pm2.yaml",
    "prod-typeorm": "tsc && APP_MODE=prod node ./node_modules/typeorm/cli.js --config src/config/ormconfig.prod",
    "start": "npm run dev-start",
    "testg": "APP_MODE=test jest --runInBand",
    "test": "APP_MODE=test jest --runInBand --setupFilesAfterEnv=./src/config/jest.setup.js",
    "postinstall": "npx tsc"
  },
  "jest": {
    "verbose": true,
    "transform": {
        "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
        "<rootDir>/src/config/",
        "<rootDir>/dist/"
    ],
    "testMatch": [
        "**/__tests__/**/*.+(ts|tsx|js)",
        "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ]
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.22",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/moxios": "^0.4.9",
    "@types/node": "^10.12.12",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^5.0.0-alpha.7",
    "helmet": "^3.21.2",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "pm2": "^3.2.9",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "sqlite3": "^4.1.0",
    "supertest": "^3.4.1",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "^24.1.0",
    "tsc-watch": "^1.0.31",
    "typeorm": "^0.2.20",
    "typescript": "^3.2.2"
  },
  "devDependencies": {}
}
