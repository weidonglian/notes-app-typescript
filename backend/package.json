{
  "name": "notesapp",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start-dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "start-prod": "tsc && APP_MODE=prod node ./dist/server.js",
    "start": "npm run start-dev",
    "test": "APP_MODE=test jest --runInBand --detectOpenHandles",
    "test-watch": "APP_MODE=test jest --runInBand --detectOpenHandles --watchAll",
    "postinstall": "npx tsc"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/src/config/",
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/config/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.22",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/moxios": "^0.4.9",
    "@types/node": "^10.12.12",
    "@types/swagger-ui-express": "^3.0.0",
    "apollo-server-express": "^2.15.1",
    "axios": "^0.19.0",
    "axiosist": "^0.7.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.9.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^6.2.0",
    "express": "^5.0.0-alpha.7",
    "helmet": "^3.21.2",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "moxios": "^0.4.0",
    "pg-promise": "^10.5.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "sqlite3": "^4.1.0",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "^24.1.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.6"
  },
  "devDependencies": {}
}